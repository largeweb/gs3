{
    "name": "gs3-turbopack",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:edge\" \"npm run dev:node\"",
        "dev:edge": "next dev --turbopack",
        "dev:node": "nodemon --ignore 'projects/*' --ignore 'templates/*' server.js",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "pages:build": "npx @cloudflare/next-on-pages",
        "preview:edge": "npm run pages:build && wrangler pages dev",
        "preview:node": "nodemon --ignore 'projects/*' --ignore 'templates/*' server.js",
        "preview": "concurrently \"npm run preview:edge\" \"npm run preview:node\"",
        "deploy": "npm run pages:build && wrangler pages deploy",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
    },
    "dependencies": {
        "@types/react-syntax-highlighter": "^15.5.13",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "framer-motion": "^11.15.0",
        "next": "15.1.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hot-toast": "^2.4.1",
        "react-syntax-highlighter": "^15.6.1"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "^1.13.7",
        "@cloudflare/workers-types": "^4.20241224.0",
        "@eslint/eslintrc": "^3",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "concurrently": "^9.1.1",
        "eslint": "^9",
        "eslint-config-next": "15.1.0",
        "nodemon": "^3.1.9",
        "postcss": "^8",
        "tailwindcss": "^3.4.1",
        "typescript": "^5",
        "vercel": "^39.2.2",
        "wrangler": "^3.99.0"
    }
}
